<h1>POST Request Received</h1>
<h2>URL Parameter Table</h2>

<table>
	<tr>
		<th>Name</th>
		<th>Value</th>
	</tr>
  {{#each query_data}}
    <tr>
    	<td>{{this.key}}</td>
    	<td>{{this.value}}</td>
    </tr>
  {{/each}}
</table>

<h2>Request Body Parameter Table</h2>
<table>
  <tr>
    <th>Name</th>
    <th>Value</th>
  </tr>
  {{#each body_data}}
    <tr>
      <td>{{this.key}}</td>
      <td>{{this.value}}</td>
    </tr>
  {{/each}}
</table>